profile docked_right_clamshell {
  output "GIGA-BYTE TECHNOLOGY CO., LTD. M32QC 22090B001125" enable position 0,0
  output eDP-1 disable position 2560,0
  exec systemctl --user restart bar
  exec systemctl --user start bar.laptop
